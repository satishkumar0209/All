package POM;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class Login {
	public static  WebElement element;
	
	public static WebElement username(WebDriver driver)

	{
		return driver.findElement(By.xpath("/html/body/app-root/app-login/div/div/div[2]/div[2]/span[2]/input"));
		
	}
	public static WebElement password(WebDriver driver)

	{
		return driver.findElement(By.cssSelector("[placeholder='Password']"));
		
		
	}
	public static WebElement loginbutton(WebDriver driver)

	{
		return driver.findElement(By.cssSelector(".//*[@type= 'submit']"));
		
		
	}
}
